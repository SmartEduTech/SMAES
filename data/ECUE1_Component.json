{
  "title": "évaluation ECUE",
  "name": "ECUE",
  "ID": "1",
  "description": "Calcule de moyenne d'elements constitutifs d'unité d'enseignement de Licence et Mastère",
  "input_variables": {
    "TP": {
      "type": "double|integer",
      "value": 17
    },
    "DS": {
      "type": "double|integer|string",
      "value": 7,
      "default": "",
      "enum": "ABS:Absence;PR:Prolongation;DIP:Disponsé,toules etudiant qui ont déjà aqui ce module"
    },
    "CC": {
      "type": "double|integer",
      "value": ""
    },
    "EXAM": {
      "type": "double|integer|string",
      "value": "ABS",
      "enum": "ABS,PR,DIP"
    },
    "EXAM_ABS": {
      "type": "boolean",
      "default": false,
      "value": ""
    },
    "CREDIT": {
      "type": "double|integer",
      "readonly": true,
      "value": 25
    },
    "SESSION": {
      "type": "string",
      "value": "principale"
    },
    "REGIME": {
      "type": "string",
      "default": "mixte",
      "value": "mixte"
    }
  },
  "output_variables": {
    "credit": {
      "type": "double|integer",
      "value": ""
    },
    "average": {
      "type": "double|integer",
      "value": ""
    }
  },
  "Rules": {
    "calculate_average": {
      "R1": {
        "Id": "Absence",
        "title": "Gestion des absences",
        "Rule_Type": "logic",
        "Evaluation": {
          "Condition": "'@EXAM'=='ABS'",
          "output": {
            "true": {
              "bind": {
                "@EXAM_ABS": "true",
                "@EXAM": 0
              }
            },
            "false": {
              "bind": {
                "@EXAM_ABS": "false"
              }
            }
          }
        }
      },
      "R2": {
        "Id": "calculer_CC",
        "title": "Calcule du controle continu",
        "Rule_Type": "arithmeticLogic",
        "Evaluation": {
          "Condition": "'@REGIME'=='mixte'",
          "output": {
            "true": {
              "bind": {
                "@CC": "Methods::{'SUM'}((Methods::{'MAX'}({subcomps:@TP*0.2})),(Methods::{'MAX'}({subcomps:@DS*0.3})))"
              }
            }
          }
        }
      }
    },
    "calculate_credits": {
      "R1": {
        "Id": "",
        "title": "",
        "Rule_Type": "",
        "Evaluation": {
          "Condition": ""
        }
      }
    }
  },
